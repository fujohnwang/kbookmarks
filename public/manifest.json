{
  "name": "kBookmarks",
  "manifest_version": 3,
  "version": "0.1.1",
  "author": "fujohnwang",
  "description": "bookmark manager chrome extension from keevol",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "bookmarks",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "icon.jpg",
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "MacCtrl+Shift+K"
      }
    }
  }
}
